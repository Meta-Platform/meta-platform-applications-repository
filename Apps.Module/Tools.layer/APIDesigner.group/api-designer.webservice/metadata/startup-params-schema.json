{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "port": {
      "type": "string",
      "pattern": "^[0-9]+$"
    },
    "serverName": {
      "type": "string"
    },
    "apisDirPath": {
      "type": "string",
      "format": "string"
    }
  },
  "required": ["port", "serverName", "apisDirPath"]
}
