          "in": "body",
          "type": "string",
          "required": true
        },
        {
          "name": "packageName",
          "in": "body",
          "type": "string",
          "required": true
        },
        {
          "name": "module",
          "in": "body",
          "type": "string",
          "required": true
        },
        {
          "name": "path",
          "in": "body",
          "type": "string",
          "required": true
        }
